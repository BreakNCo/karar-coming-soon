{
  "name": "karar-coming-soon",
  "description": "karar Coming Soon website",
  "version": "0.0.1",
  "private": true,
  "repository": "https://github.com/BreakNCo/karar-coming-soon",
  "license": "MIT",
  "scripts": {
    "build": "npm run css",
    "css-compile": "sass --style compressed --source-map --embed-sources --no-error-css --load-path=node_modules scss/:css/",
    "css-lint": "stylelint scss/",
    "css-prefix": "postcss --replace css/styles.css --use autoprefixer --map",
    "css": "npm-run-all css-compile css-prefix",
    "server": "sirv --dev --no-clear --port 3000",
    "start": "npm-run-all --parallel watch server",
    "watch": "nodemon -e html,js,scss -x \"npm run css\"",
    "test": "npm-run-all css-lint css"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "jquery": "^3.7.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "nodemon": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.44",
    "postcss-cli": "^11.0.0",
    "sass": "^1.77.8",
    "sirv-cli": "^2.0.2",
    "stylelint": "^16.9.0",
    "stylelint-config-twbs-bootstrap": "^15.0.0"
  }
}
